<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
		"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
		"http://struts.apache.org/dtds/struts-2.1.7.dtd">

<struts>
  <constant name="struts.devMode" value="true"/>

	<package name="userManage"  extends="struts-default">
		<action name="login" class="action.UserAction" method="login">
			<result name="input">/page/login.jsp</result>
			<result name="success">/page/success.jsp</result>
		</action>
		<action name="register" class="action.UserAction" method="register">
			<result name="input">/page/register.jsp</result>
			<result name="success">/page/success.jsp</result>
		</action>
	</package>
	
</struts>